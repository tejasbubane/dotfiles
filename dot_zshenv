export PATH="/usr/local/opt/php71/bin:$PATH"

# Yarn bin paths
export PATH="$HOME/.yarn/bin:$PATH"
export PATH="$PATH:/usr/local/Cellar/node/8.6.0/bin"

# Python user base binary directory
# Especially for using pipenv
export PATH="$PATH:/Users/tejas/Library/Python/2.7/bin"

# GO paths
export GOPATH=$HOME/go
export PATH=$PATH:$GOPATH/bin
export PATH=$PATH:/usr/local/go/bin
ulimit -n 8096

